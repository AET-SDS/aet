<suite name="aet-json-test" company="aet" domain="http://127.0.0.1:8181/" project="aet">
    <test name="aet-test-suite-result-metadata-JSON">
        <collect>
            <open/>
            <source/>
        </collect>
        <compare>
            <source comparator="source">
                <remove-regexp regExp='\"correlationId\": \".*\"'/>
                <remove-regexp regExp='\"artifactId\": \".*\"'/>
                <remove-regexp regExp='\"collectTimestamp\": \".*\",'/>

                <remove-regexp regExp='\"version\": \d*'/>
                <remove-regexp regExp='\"value\": \d*'/>
            </source>
        </compare>
        <urls>
            <!-- test suite result JSON to be tested: -->
            <url href="api/metadata?company=aet&project=aet&suite=main&formatted=true"/>
        </urls>
    </test>
</suite>
