<?xml version="1.0" encoding="UTF-8"?>
<suite name="TS1" company="Cognifide" environment="win7-ff16">
	<test name="test1">
		<collect>
			<open/>
			<sleep duration="1500"/>
			<source/>
			<modify-cookie action="add" cookie-name="analytics" cookie-value="test-value"/>
			<cookie/>
