			<cookie name="list"/>
			<cookie action="compare" name="compare-cookies"/>
			<cookie action="test" cookie-name="analytics" name="test-analytics-cookie"/>
		</compare>
		<urls>
			<url href="http://www.cognifide.com"/>
		</urls>
	</test>
	<report/>
</suite>